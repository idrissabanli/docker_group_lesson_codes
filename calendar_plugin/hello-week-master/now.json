{
  "version": 2,
  "name": "hello-week",
  "builds": [
    { "src": "docs/**/*.md", "use": "@now/static" },
    { "src": "docs/**/demos/*.html", "use": "@now/static" },
    { "src": "docs/**/demos/**/*.js", "use": "@now/static" },
    { "src": "docs/**/demos/**/*.css", "use": "@now/static" },
    { "src": "docs/**/demos/**/*.json", "use": "@now/static" },
    { "src": "dist/*.js", "use": "@now/static" },
    { "src": "dist/*.css", "use": "@now/static" },
    { "src": "dist/langs/*.js", "use": "@now/static" },
    { "src": "static/vivaldi/vivaldi.min.js", "use": "@now/static" },
    { "src": "static/images/*.png", "use": "@now/static" },
    { "src": "static/styles/*.css", "use": "@now/static" },
    { "src": "*.html", "use": "@now/static" },
    { "src": "vivaldi.config.js", "use": "@now/static" },
    { "src": "README.md", "use": "@now/static" },
    { "src": "manifest.json", "use": "@now/static" }
  ],
  "routes": [{ "src": "/", "dest": "index.html" }]
}
